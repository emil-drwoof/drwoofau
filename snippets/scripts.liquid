          {% else %}
            <script src="{{ 'theme-preorder.js' | asset_url }}" defer></script>
          {% endif %}
        {% else %}
          <script src="{{ 'theme.js' | asset_url }}" defer></script>
        {% endif %}
      {% endif %}
