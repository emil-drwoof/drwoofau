                          {%- endif -%}
                        {%- else -%}
                          {{ 'products.product.waiting_for_stock' | t }}
                        {%- endif -%}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- endunless -%}
              {% endif %}


